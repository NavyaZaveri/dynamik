class Math() {
    fn abs(x) {
        if (x<=0) { return -x;}
        return x;
     }
     fn add(x,y) { return x+y;}
     fn sub(x,y) {return x-y;}
     fn sum_list(lst) {
        var i = 0;
        var sum = 0;
        while (i<lst.len()) {sum = sum+lst.get(i); i = i+1;}
        return sum;
     }
}


